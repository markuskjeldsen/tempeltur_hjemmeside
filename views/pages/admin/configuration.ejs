
<%- include('../../common/head') %>


<style>
    table, th, td {
      border: 1.5px solid black;
    }
    #textboxid
    {
    height:500px;
    font-size:14pt;
    }
</style>

<body>
    <header>
        <%- include('../../common/header') %>
    </header>
    


    <main>

        <div class="container">
            <div class="col col-md-12">

                <h2> Tempeltur tekst </h2>


                <form action="/admin/configuration" method="post">
                    <h4> al tekst </h4>

                    <br>
                    <label for="fname">forside:</label> <br>
                    <textarea name="forside" rows="4" cols="80"><%= data.res.locals.config.forside%></textarea>
                    <br>
                    <label for="fname">tilmeldning:</label><br>
                    <textarea name="tilmeldning" rows="4" cols="80"><%= data.res.locals.config.tilmeldning %></textarea>
                    <br>
                    <label for="fname">program tekst</label><br>
                    <textarea name="program" rows="4" cols="80"><%= data.res.locals.config.program %></textarea>
                    <br>
                    <label for="fname">praktsk information</label><br>
                    <textarea name="info" rows="4" cols="80"><%= data.res.locals.config.info %></textarea>
                    <br>
                    <input type="submit" value="submit">
                </form> 




                
            </div>
        </div>
    </main>



    <footer>
        <%- include('../../common/footer') %>
    </footer>


</body>
</html>
